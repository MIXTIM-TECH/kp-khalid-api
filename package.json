{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "sass_prod": "sass --no-source-map --style=compressed resources/sass/app.sass public/css/app.css",
        "sass_dev": "sass --no-source-map resources/sass/app.sass public/css/app.css",
        "sass_w": "sass --no-source-map --style=compressed resources/sass/app.sass public/css/app.css -w"
    },
    "devDependencies": {
        "axios": "^0.25",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "bootstrap": "^5.2.3",
        "sweetalert2": "^11.7.5"
    }
}
